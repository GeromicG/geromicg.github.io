{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Loop Dashboard Chart",
   "title": {
    "text": "78 : Employment activities: Weights:CVM",
    "subtitle": "Source: ONI",
    "anchor": "start"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/GeromicG/geromicg.github.io/refs/heads/main/l4n2.json",
    "format": {"type": "json", "property": "years"}
  },
  "width": 300,
  "height": 225,
  "mark": {
    "type": "line",
    "point": true
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "title": "Year",
      "axis": {
        "grid": false
      }
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": "Value",
      "axis": {
        "grid": false
      }
    },
    "color": {
      "value": "#1A73E8" 
    }
  },
  "config": {
    "background": "#f9f9f9",
    "title": {
      "fontSize": 11,
      "font": "sans-serif",
      "anchor": "start",
      "color": "#333"
    },
    "axis": {
      "labelFontSize": 10,
      "titleFontSize": 10,
      "titleFont": "sans-serif",
      "labelFont": "sans-serif",
      "labelColor": "#333",
      "titleColor": "#333"
    }
  }
}
